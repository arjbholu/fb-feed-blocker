{
  "manifest_version": 2,

  "name": "FB feed",
  "description": "fuck",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "css": ["fb.css"],
      "all_frames":true
    } 
  ]
}
